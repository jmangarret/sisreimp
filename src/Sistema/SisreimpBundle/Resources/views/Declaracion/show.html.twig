{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Declaracion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechadesde</th>
                <td>{{ entity.fechaDesde|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechahasta</th>
                <td>{{ entity.fechaHasta|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('declaracion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('declaracion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
