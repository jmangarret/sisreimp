{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ActividadesEmpresa list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Declaracion</th>
                <th>Actividad</th>
                <th>Ingresobruto</th>                                
                <th>Status</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('actividadesempresa_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
				<td>{{ entity.idDeclaracion }}</td>	
				<td>{{ entity.idActividad }}</td>                
                <td>{{ entity.ingresoBruto }}</td>
                <td>{{ entity.status }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('actividadesempresa_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('actividadesempresa_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('actividadesempresa_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
